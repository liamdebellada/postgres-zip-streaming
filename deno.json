    "start": "deno run -A --env-file src/main.ts",
    "dev": "deno run -A --env-file --watch src/main.ts",
    "check": "deno check . && deno lint . && deno fmt --check"
  },
  "imports": {
    "pg": "npm:pg@8.13.1",
    "zod": "npm:zod@3.24.1",
    "pg-query-stream": "npm:pg-query-stream@4.7.1",
    "@types/pg": "npm:@types/pg@8.11.11",
    "@zip-js/zip-js": "npm:@zip.js/zip.js"
  },
  "nodeModulesDir": "manual"
}
